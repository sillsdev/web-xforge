{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run and debug current Deno script",
      "type": "node",
      "request": "launch",
      "cwd": "${fileDirname}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect-wait", "--allow-all", "${file}"],
      "attachSimplePort": 9229,
      "outputCapture": "std"
    }
  ]
}
