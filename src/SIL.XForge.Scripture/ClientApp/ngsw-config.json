{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js", "/manifest.json"]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "!/assets/i18n/**"]
      }
    },
    {
      "name": "i18n",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/i18n/**"]
      }
    },
    {
      "name": "fonts",
      "installMode": "prefetch",
      "resources": {
        "urls": ["https://fonts.googleapis.com/*", "https://fonts.gstatic.com/*"]
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/",
    "!/terms",
    "!/privacy",
    "!/learn-more",
    "!/language-api",
    "!/connect/**",
    "!/identity-api/**",
    "!/json-api/**",
    "!/paratext-api/**",
    "!/sharedb/**",
    "!/.well-known/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ]
}
